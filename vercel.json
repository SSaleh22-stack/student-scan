{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/[[...path]].ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[[...path]]?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

